# Server Configuration
PORT=3000
HOST=0.0.0.0

# Google Authentication
# Path to your Google OAuth credentials JSON file
GOOGLE_APPLICATION_CREDENTIALS=./google_oauth_creds.json

# Use Google Cloud Authentication
GOOGLE_GENAI_USE_GCA=true

# Google Cloud Access Token (from OAuth credentials)
GOOGLE_CLOUD_ACCESS_TOKEN=your_access_token_here

# CLI Configuration
CLI_VERSION=1.0.0
DEBUG=false

# Model Configuration
DEFAULT_MODEL=gemini-2.0-flash-exp
DEFAULT_EMBEDDING_MODEL=text-embedding-004

# Target Directory
TARGET_DIR=./